{
  "name": "angular-current-showcase",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "dev": "npm run start -- --open",
    "ng": "ng",
    "prestart:e2e": "npm run build",
    "start": "ng serve --serve-path angular-current-showcase",
    "start:e2e": "npx http-server ../../build-showcases",
    "test": "ng test",
    "test:e2e": "cross-env showcase=angular-current-showcase npx playwright test --config=../playwright.config.ts",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "15.2.0",
    "@angular/common": "15.2.0",
    "@angular/compiler": "15.2.0",
    "@angular/core": "15.2.0",
    "@angular/forms": "15.2.0",
    "@angular/platform-browser": "15.2.0",
    "@angular/platform-browser-dynamic": "15.2.0",
    "@angular/router": "15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.0",
    "@angular/cli": "15.2.0",
    "@angular/compiler-cli": "15.2.0",
    "@playwright/test": "1.30.0",
    "cross-env": "^7.0.3",
    "schematics-scss-migrate": "^1.3.15",
    "typescript": "4.9.5"
  }
}
